<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.prashant.musicvideosample">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.prashant.musicvideosample.base.MusicVideoApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.activity.neww"></activity>
        <activity android:name="com.prashant.musicvideosample.ui.activity.catagrey" />
        <activity android:name="com.prashant.musicvideosample.ui.activity.profile" />
        <activity android:name="com.prashant.musicvideosample.ui.activity.shopp" />
        <activity android:name=".ui.activity.shop" />
        <activity android:name="com.prashant.musicvideosample.ui.activity.secondHome" />
        <activity android:name="com.prashant.musicvideosample.ui.activity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>